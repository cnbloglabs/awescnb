{
  "name": "tona-vite",
  "type": "module",
  "version": "0.0.2",
  "description": "Vite plugin for Tona themes - combines dynamic script extension and shared assets serving",
  "author": {
    "name": "guangzan",
    "url": "https://www.cnblogs.com/guangzan",
    "email": "guangzan1999@outlook.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/cnbloglabs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cnbloglabs/tona.git"
  },
  "bugs": {
    "url": "https://github.com/cnbloglabs/tona/issues"
  },
  "keywords": [
    "vite",
    "plugin",
    "dynamic-script",
    "shared-assets",
    "博客园"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.mts"
      ]
    }
  },
  "files": [
    "dist",
    "public"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown"
  },
  "peerDependencies": {
    "vite": "^5.0.0 || ^6.0.0 || ^7.0.0"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "tsdown": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:"
  }
}
